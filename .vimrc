call pathogen#infect()
call pathogen#helptags()
syntax on
filetype plugin indent on
au BufNewFile,BufRead *.wsdl set filetype=xml

set nocompatible

set t_Co=256

set tabstop=2
set shiftwidth=2
set expandtab

set autoindent
set smarttab

set wrap
set number
set ruler

set lines=50 columns=90

set title
set hlsearch
